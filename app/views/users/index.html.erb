All Users Shown Here

<ul class="users" style="list-style: none;">
  <% @users.each do |user| %>
    <li>
      <%#= gravatar_for user, size: 50 %>
      <%#= link_to gravatar_for(user), user %>
      <%= link_to gravatar_for(user), user %>
      id: <%= user.id %>,
      name: <%= user.name %>,
      email: <%= user.email %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>