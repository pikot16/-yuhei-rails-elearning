<h4 class="text-center">Word Registration</h4>
<%= form_for([:admin,@category,@word]) do |f| %> <%# adminの中のcategoryの中のword %>
    <%= render "shared/error_messages", object: f.object %>
    <div class="form-group">
        <%= f.label :content, "Word" %> <%# wordに変える %>
        <%= f.text_field :content, class: "form-control" %>

        <%= f.fields_for :choices do |c| %>
            <%= c.label :content, "Choice" %>
            <%= c.text_field :content, class: "form-control" %>
            <%= c.label :correct %>
            <%= c.check_box :correct %><br>
        <% end %> <%# このendがないとエラーになる %>
    </div>
    <div class="text-center">
        <%= link_to "Back", admin_categories_path, class: "btn btn-warning" %>
        <%= f.submit "Save Word", class: "btn btn-primary" %>
    </div>
<% end %>